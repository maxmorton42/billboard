<h1><%= @artist.id ? 'Edit Artist' : 'Create Artist' %></h1>
<hr />
<%= form_for [@billboard, @artist] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <br />
  <%= f.label :genre %>
  <%= f.text_area :genre, required: true %>
  <%= f.label :bio %>
  <%= f.text_area :bio %>

  <%= f.submit %>
<% end %>